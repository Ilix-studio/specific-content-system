{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/specific-content-system/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/specific-content-system/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/specific-content-system",
  "projects": {
    "specific-content-system": {
      "type": "application",
      "root": "apps/specific-content-system",
      "entryFile": "main",
      "sourceRoot": "apps/specific-content-system/src",
      "compilerOptions": {
        "tsConfigPath": "apps/specific-content-system/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "intitute": {
      "type": "application",
      "root": "apps/intitute",
      "entryFile": "main",
      "sourceRoot": "apps/intitute/src",
      "compilerOptions": {
        "tsConfigPath": "apps/intitute/tsconfig.app.json"
      }
    },
    "institute": {
      "type": "application",
      "root": "apps/institute",
      "entryFile": "main",
      "sourceRoot": "apps/institute/src",
      "compilerOptions": {
        "tsConfigPath": "apps/institute/tsconfig.app.json"
      }
    },
    "content": {
      "type": "application",
      "root": "apps/content",
      "entryFile": "main",
      "sourceRoot": "apps/content/src",
      "compilerOptions": {
        "tsConfigPath": "apps/content/tsconfig.app.json"
      }
    },
    "student": {
      "type": "application",
      "root": "apps/student",
      "entryFile": "main",
      "sourceRoot": "apps/student/src",
      "compilerOptions": {
        "tsConfigPath": "apps/student/tsconfig.app.json"
      }
    },
    "teacher": {
      "type": "application",
      "root": "apps/teacher",
      "entryFile": "main",
      "sourceRoot": "apps/teacher/src",
      "compilerOptions": {
        "tsConfigPath": "apps/teacher/tsconfig.app.json"
      }
    },
    "user-profile": {
      "type": "application",
      "root": "apps/user-profile",
      "entryFile": "main",
      "sourceRoot": "apps/user-profile/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-profile/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}